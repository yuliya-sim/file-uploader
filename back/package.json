{
  "name": "back",
  "version": "1.0.0",
  "description": "",
  "main": "src/app.js",
  "scripts": {
    "start": "node  src/app.js",
    "start:dev": "nodemon src/app.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "type": "module",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "eslintConfig": {
    "extends": "airbnb-base",
    "env": {
